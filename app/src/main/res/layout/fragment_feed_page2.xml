<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:id="@+id/linearLayout4"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/feedPageFrame2Left"
            android:layout_width="4.3dp"
            android:layout_height="8.9dp"
            android:src="@drawable/feedpage36" />

        <TextView
            android:id="@+id/feedPageDate"
            android:layout_width="wrap_content"
            android:layout_height="19dp"
            android:layout_marginLeft="33dp"
            android:layout_marginRight="33dp"
            android:fontFamily="@font/nanumbarungothic"
            android:lineSpacingExtra="3sp"
            android:text="2019년ㆍ10월"
            android:textColor="#000000"
            android:textSize="16sp" />

        <ImageView
            android:id="@+id/feedPageFrame2Right"
            android:layout_width="4.3dp"
            android:layout_height="8.9dp"
            android:src="@drawable/feedpage37" />

    </LinearLayout>

<!--    <android.support.v7.widget.RecyclerView
        android:id="@+id/fragmentItem"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:scrollbars="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout4" />-->

</androidx.constraintlayout.widget.ConstraintLayout>